#include<stdio.h>
#include<conio.h>
int main()
{
    int a, i;
    printf("Enter the limit of a: ");
    scanf("%d", &a);
    printf("\n");
    for(i=1; i<=a; i++)
    {
        if(i==a)
            printf("%d", i);
        else
            printf("%d,", i);
    }
    getch();
    return 0;
}

OUTPUT:-
Enter the limit of a: 6
1,2,3,4,5,6
